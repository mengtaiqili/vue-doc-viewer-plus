    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "ant-design-vue": "^4.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.1.10"
